	"keywords": [
		"react",
		"tanstack"
	],
	"author": "Shinya Fujino <shf0811@gmail.com> (https://github.com/morinokami)",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/morinokami/tanstack-meta.git",
		"directory": "packages/meta"
	},
	"bugs": {
		"url": "https://github.com/morinokami/tanstack-meta/issues"
	},
	"exports": {
